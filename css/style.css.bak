@charset "utf-8";

.positoinNum{
	position:fixed;
	left:50%;
	top:50%;
	width:400px;
	height:100px;
	margin:-50px 0 0 -200px;
	background:#333;
	line-height:100px;
	font-size:20px;
	color:#fff;
	font-weight:bold;
	text-align:center;
	z-index:999;
}

img{vertical-align:top;}
.scrollbox{height:900px;position:relative;overflow:hidden;}
.scrollbox > p{position:absolute;left:50%;}
.scrollbox > p img{position:relative;}
.txt01,
.txt02,
.txt03,
.txt04{overflow:hidden;}
.baloon01 img,
.baloon02 img,
.baloon03 img,
.txt01 img,
.txt02 img,
.txt03 img,
.txt04 img{position:relative;display:block;}





